<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="btnmsg" alignment="CENTER" prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MenuController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
   <children>
      <Button fx:id="NewGameBtn" alignment="CENTER" mnemonicParsing="false" onAction="#pressNewGameButton" prefHeight="60.0" prefWidth="110.0" text="New Game" translateY="-120.0" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
      <Button fx:id="ContinueBtn" alignment="CENTER" mnemonicParsing="false" onAction="#pressContinueButton" prefHeight="60.0" prefWidth="110.0" text="Continue" translateY="-60.0" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
      <Button fx:id="contbtn" alignment="CENTER" mnemonicParsing="false" onAction="#pressControlerButton" prefHeight="60.0" prefWidth="110.0" text="Controler" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
      <Button fx:id="credbtn" alignment="CENTER" mnemonicParsing="false" onAction="#pressCreditsButton" prefHeight="60.0" prefWidth="110.0" text="Credits" translateY="60.0" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
      <Button fx:id="extbtn" alignment="CENTER" layoutX="206.0" layoutY="180.0" mnemonicParsing="false" onAction="#pressExitButton" prefHeight="60.0" prefWidth="110.0" text="Exit" translateY="120.0" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
   </children>
</GridPane>
